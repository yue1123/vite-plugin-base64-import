{
  "name": "vite-plugin-data-url-query",
  "version": "1.0.0",
  "description": "Encoding img to base64 with base64 import query",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "pnpm clean && tsup src/index.ts  --watch --dts --format esm",
    "build": "pnpm clean && tsup src/index.ts --dts --env.NODE_ENV production --format esm && pnpm clean-up",
    "clean": "rimraf dist",
    "clean-up": "rimraf tsconfig.tsbuildinfo",
    "example:dev": "pnpm run -C example dev",
    "example:build": "pnpm run -C example build",
    "example:preview": "pnpm run -C example preview"
  },
  "files": [
    "types.d.ts",
    "screenshots",
    "dist"
  ],
  "keywords": [
    "vite",
    "vite-plugin",
    "plugin",
    "base64",
    "base64 import",
    "import query",
    "img",
    "image",
    "data-url",
    "data-uri",
    "static assets"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/html-minifier": "^4.0.2",
    "@types/node": "^18.15.0",
    "@types/uglify-js": "^3.17.1",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.2.0"
  },
  "peerDependencies": {
    "vite": "*"
  },
  "dependencies": {
    "mime": "^4.0.1"
  }
}
